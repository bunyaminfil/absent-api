"SELECT users.id, lesson.lesson, day.day, program.absent, program.hour from absent.users INNER JOIN absent.program ON users.id = program.userid INNER JOIN absent.day ON program.dayid = day.id INNER JOIN absent.lesson ON program.lessonid = lesson.id";

var q = "SELECT program.userid, program.id, lesson.lesson, day.day, program.absent, program.hour" + 
" from absent.program INNER JOIN absent.day ON program.dayid = day.id INNER JOIN" + 
" absent.lesson ON program.lessonid = lesson.id INNER JOIN absent.users ON program.userid = users.id";

mysql assions relation session
classMethods: {
    associate: function (models) {
      User.belongsToMany(models.Project, { 
        through: 'UserProject',
        foreignKey: 'userId'
      })
    }
  },
  freezeTableName: true
})
  return User
}

 User.associate = function (models) {
   User.belongsToMany(models.Project, { 
    through: 'UserProject',
    foreignKey: 'userId'
   })
 };
 return User
}